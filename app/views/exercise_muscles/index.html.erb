<p id="notice"><%= notice %></p>

<h1>Exercise Muscles</h1>

<table>
  <thead>
    <tr>
      <th>E</th>
      <th>M</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exercise_muscles.each do |exercise_muscle| %>
      <tr>
        <td><%= exercise_muscle.e_id %></td>
        <td><%= exercise_muscle.m_id %></td>
        <td><%= link_to 'Show', exercise_muscle %></td>
        <td><%= link_to 'Edit', edit_exercise_muscle_path(exercise_muscle) %></td>
        <td><%= link_to 'Destroy', exercise_muscle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exercise Muscle', new_exercise_muscle_path %>
